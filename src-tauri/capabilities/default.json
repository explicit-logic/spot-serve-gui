{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:allow-open",
    "fs:allow-app-read",
    "fs:allow-app-read-recursive",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "binaries/cloudflared/cloudflared",
          "sidecar": true,
          "args": true
        }
      ]
    },
    "shell:allow-open",
    "shell:allow-spawn"
  ]
}
